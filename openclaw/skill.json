{
  "openclaw": {
    "emoji": "ðŸ“ˆ",
    "requires": {
      "bins": ["standx"]
    },
    "primaryCredential": {
      "kind": "env",
      "env": "STANDX_JWT",
      "description": "StandX JWT token from https://standx.com/user/session (valid 7 days)"
    },
    "optionalEnvVars": [
      {
        "name": "STANDX_PRIVATE_KEY",
        "description": "Ed25519 private key (Base58) for trading operations",
        "sensitive": true
      }
    ],
    "homepage": "https://github.com/wjllance/standx-cli",
    "source": {
      "type": "github",
      "owner": "wjllance",
      "repo": "standx-cli",
      "verified": true
    },
    "install": [
      {
        "id": "brew",
        "kind": "brew",
        "formula": "wjllance/standx-cli/standx-cli",
        "bins": ["standx"],
        "label": "Install StandX CLI via Homebrew",
        "verification": "https://github.com/wjllance/standx-cli/blob/main/homebrew/standx-cli.rb"
      }
    ]
  }
}
